// .js

import fetch from 'node-fetch'

async function getNama() {
    try {
        const data = await fetch('https://jsonplaceholder.typicode.com/users')
        const data2 = await data.json()
        data2.map((item)=> {
            console.log(`name: ${item.name}`) 
        })
    } catch (error) {
        console.log(error) 
    }
}

getNama()

